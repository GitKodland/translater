meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "РOФЛ": "Подкол",
            "КРИПОВЫЙ": "Страшный",
            "АГРИТЬСЯ": "Злится"
                }
            
word = input("Введите непонятное слово (большими буквами!): ").upper()

if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("Упс я оказывается и сам дед")
